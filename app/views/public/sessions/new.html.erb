<div class="container">
<h2>ログイン</h2>
<%= form_with model: @user, url: new_user_session_path do |f| %>
<div><%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "メールアドレス" %></div>
<div><%= f.password_field :password, autocomplete: "current-password", placeholder: "パスワード" %></div>
<%= f.submit "ログイン" %>
<% end %>
</div>